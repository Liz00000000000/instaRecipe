<h1 class='tile rec'> Type of Cuisine</h1>

<ul class = 'tile rec'>
    <% @cuisines.each do |i| %>
        <%if i.name == nil %>
            <%= link_to "confort food", cuisine_path(i) %></li>
        <% else %>
            <li><%= link_to i.name, cuisine_path(i) %></li>
        <% end %>
    <%end%>

</ul>